{
    "manifest_version": 3,
    "name": "Get to the Recipe!",
    "version": "1.0",
    "description": "Skip past all the waffle and get to the actual recipe on a website.",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"],
        "run_at": "document_end"
      }
    ],
    "icons": {
      "48": "./logo.png",
      "96": "./logo.png",
      "128": "./logo.png"
    }
  }